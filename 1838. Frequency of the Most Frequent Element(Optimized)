class Solution {
    public int maxFrequency(int[] nums, int k) {
        Arrays.sort(nums);
        int max = 0;
        long sum = 0;
        int i = 0;

        for(int j = 0; j < nums.length; j++) {
            sum = sum + nums[j];
            while((long)(j-i+1) * nums[j] - sum > k){
                sum = sum - nums[i];
                i++;
            
            }
            max = Math.max(max,j-i+1);
        }
        return max;
        
    }
}
